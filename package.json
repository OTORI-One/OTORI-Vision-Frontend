{
  "name": "otori-vision",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend",
    "program"
  ],
  "scripts": {
    "frontend:dev": "npm run dev --workspace=frontend",
    "frontend:build": "npm run build --workspace=frontend",
    "frontend:start": "npm run start --workspace=frontend",
    "backend:dev": "npm run dev --workspace=backend",
    "backend:start": "npm run start --workspace=backend",
    "program:build": "cd program && cargo build",
    "program:test": "cd program && cargo test",
    "dev": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\"",
    "build": "npm run frontend:build && npm run program:build",
    "start": "concurrently \"npm run frontend:start\" \"npm run backend:start\"",
    "test": "npm run test --workspace=frontend && npm run program:test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.1.1",
    "@omnisat/lasereyes": "^0.0.140",
    "@omnisat/lasereyes-core": "^0.0.62",
    "@omnisat/lasereyes-react": "^0.0.57",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.7",
    "lasereyes-mono": "github:omnisat/lasereyes-mono#main",
    "next": "13.5.6",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/recharts": "^1.8.29",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/testing-library__react": "^10.2.0",
    "check-peer-dependencies": "^4.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.3.3",
    "concurrently": "^8.0.0"
  }
}
